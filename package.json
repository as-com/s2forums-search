{
  "name": "s2forums-search",
  "description": "Search engine for MIT's Scratch Forums.",
  "version": "0.6.9-beta",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/as-com/s2forums-search.git"
  },
  "homepage": "https://github.com/as-com/s2forums-search",
  "keywords": [],
  "main": "dist/server.js",
  "scripts": {
    "start": "node node_modules/forever/bin/forever --minUptime 1000 --spinSleepTime 1000 -c \"node --harmony\" ./dist/server.js",
    "build-server": "node node_modules/webpack/bin/webpack --colors --display-error-details --config configs/webpack.server.js",
    "build-client": "node node_modules/webpack/bin/webpack --colors --display-error-details --config configs/webpack.client.js",
    "build": "node node_modules/concurrently/src/main \"npm run build-server\" \"npm run build-client\"",
    "watch-server": "node node_modules/webpack/bin/webpack --watch --verbose --colors --display-error-details --config configs/webpack.server-watch.js",
    "watch-server-start": "node node_modules/just-wait --pattern \"dist/webpack-assets.json\" && npm run start",
    "watch-client": "node node_modules/webpack-dev-server/bin/webpack-dev-server --config configs/webpack.client-watch.js",
    "watch": "node node_modules/concurrently/src/main --kill-others \"npm run watch-server-start\" \"npm run watch-server\" \"npm run watch-client\""
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "body-parser": "^1.14.2",
    "elasticsearch": "^10.0.1",
    "express": "^4.13.3",
    "express-cache-response-directive": "^1.0.0",
    "forever": "^0.15.1",
    "history": "^1.17.0",
    "lodash": "^3.10.1",
    "memwatch-next": "^0.2.10",
    "moment": "^2.10.6",
    "moment-timezone": "^0.5.0",
    "newrelic": "^1.25.1",
    "react": "^0.14.6",
    "react-bootstrap": "^0.28.2",
    "react-document-title": "^2.0.1",
    "react-dom": "^0.14.6",
    "react-router": "^1.0.3",
    "react-router-bootstrap": "^0.19.3",
    "socketcluster-client": "^4.1.0",
    "socketcluster-server": "^4.1.1",
    "style-loader": "^0.13.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.2.0",
    "autoprefixer": "^6.3.1",
    "babel": "^6.3.26",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "concurrently": "^1.0.0",
    "css-loader": "^0.23.1",
    "cssnano": "^3.4.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "just-wait": "^1.0.3",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "node-sass": "^3.4.2",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.8.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "wait-run": "^1.1.0",
    "webpack": "^1.12.10",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.6.0"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
