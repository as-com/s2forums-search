{
  "name": "s2forums-search",
  "description": "Search engine for MIT's Scratch Forums.",
  "version": "0.5.3-beta",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/as-com/s2forums-search.git"
  },
  "homepage": "https://github.com/as-com/s2forums-search",
  "keywords": [],
  "main": "babel.server.js",
  "scripts": {
    "start": "NODE_PATH=\"./src\" node --harmony ./babel.server",
    "build": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.client.js",
    "watch-client": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.client-watch.js && node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.client-watch.js",
    "watch": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run watch-client\" \"npm run start\""
  },
  "dependencies": {
    "babel": "6.3.26",
    "babel-core": "6.1.19",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "body-parser": "^1.14.2",
    "browserify-zepto": "^1.1.4",
    "elasticsearch": "^10.0.1",
    "express": "^4.13.3",
    "express-cache-response-directive": "^1.0.0",
    "fetch-plus": "^3.6.1",
    "fetch-plus-json": "^3.6.0",
    "history": "1.17.0",
    "isomorphic-fetch": "^2.2.0",
    "lodash": "^3.10.1",
    "memwatch-next": "^0.2.10",
    "moment": "^2.10.6",
    "moment-timezone": "^0.5.0",
    "node-fetch": "^1.3.3",
    "piping": "0.3.0",
    "react": "0.14.4",
    "react-bootstrap": "^0.28.1",
    "react-document-title": "^2.0.1",
    "react-dom": "0.14.4",
    "react-router": "^1.0.3",
    "react-router-bootstrap": "^0.19.3",
    "react-spinner": "^0.2.3",
    "react-transmit": "3.1.5",
    "socketcluster-client": "^4.1.0",
    "socketcluster-server": "^4.1.1",
    "style-loader": "^0.13.0"
  },
  "devDependencies": {
    "babel-loader": "6.2.0",
    "concurrently": "1.0.0",
    "css-loader": "^0.23.1",
    "json-loader": "0.5.4",
    "react-hot-loader": "1.3.0",
    "sass-loader": "^3.1.2",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
